<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1dp"/>
            <gradient android:startColor="@color/goal_Progress_bar_background_greyLight" android:endColor="@color/goal_Progress_bar_background_greyLight" android:angle="270" android:centerY="0.75" android:centerColor="@color/goal_Progress_bar_background_greyLight"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1dp"/>
                <gradient android:startColor="@color/fragment_exercise_background_gradient_end" android:endColor="@color/fragment_exercise_background_gradient_end" android:angle="270" android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
</layer-list>
