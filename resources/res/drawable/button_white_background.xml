<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/black_90_percent"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/black_10_percent"/>
            <stroke android:width="1dp" android:color="@color/black_90_percent"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/black_90_percent"/>
        </shape>
    </item>
</selector>
